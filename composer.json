{
    "name": "arokettu/ip-address-doctrine",
    "description": "Doctrine support for arokettu/ip-address",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ip", "ip address", "ip block", "ip range", "ipv4", "ipv6",
        "doctrine", "mariadb", "postgresql", "postgres"
    ],
    "homepage": "https://sandfox.dev/php/ip-address-doctrine.html",
    "authors": [{
        "name": "Anton Smirnov",
        "email": "sandfox+composer@sandfox.me",
        "role": "developer",
        "homepage": "https://sandfox.me/"
    }],
    "support": {
        "issues": "https://gitlab.com/sandfox/ip-address-dcotrine/-/issues",
        "source": "https://gitlab.com/sandfox/ip-address-dcotrine",
        "docs": "https://arokettu-ip-address-doctrine.readthedocs.io/",
        "chat": "https://gitter.im/arokettu/community"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\IP\\Doctrine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\IP\\Doctrine\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "arokettu/ip-address": "^2.0",
        "arokettu/is-resource": "^1.0",
        "doctrine/dbal": "^4.2",
        "psy/psysh": "*"
    },
    "require-dev": {
        "doctrine/orm": "^3.3",
        "sandfox.dev/code-standard": "^1.2024.07.20",
        "squizlabs/php_codesniffer": "*"
    }
}
